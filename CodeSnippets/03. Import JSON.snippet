<?xml version="1.0" encoding="utf-8" ?>
<CodeSnippets  xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>Import JSON</Title>
			<Shortcut>importjson</Shortcut>
			<Description>Code snippet for importing JSON</Description>
			<Author>Microsoft Corporation</Author>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal>
					<ID>name</ID>
					<ToolTip>Property type</ToolTip>
					<Default>name</Default>
				</Literal>
				<Literal>
					<ID>items</ID>
					<ToolTip>Property type</ToolTip>
					<Default>User[]</Default>
				</Literal>
				<Literal>
					<ID>inputJson</ID>
					<ToolTip>Variable name</ToolTip>
					<Default>inputJson</Default>
				</Literal>
			</Declarations>
			<Code Language="csharp"><![CDATA[var $name$ = JsonConvert.DeserializeObject<$items$>($inputJson$);$end$]]>
			</Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>